{
  "name": "binpackr",
  "description": "The core package to create a schema object to encode/decode your JSON in to a compact byte buffer with no overhead.",
  "version": "0.2.6",
  "keywords": [
    "binary",
    "packet",
    "pack",
    "packer",
    "encode",
    "decode",
    "serialize",
    "serializer",
    "marshal",
    "schema",
    "codec",
    "buffer",
    "protocol",
    "proto"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "ty",
  "license": "MIT",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "postbuild": "node ./scripts/esbuild",
    "clean": "lb-clean binpackr*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs  \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha --allow-console-logs  \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf binpackr*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "ts-essentials": "^8.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/node": "^16.7.2",
    "buffer": "^6.0.3",
    "esbuild": "^0.12.22"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
